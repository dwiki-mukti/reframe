// const path = require('path')
const fs = require('fs')

fs.readFile('aaa/proto.ts', 'utf-8', function (err, data) {
    if (err) throw err;
    console.log(__dirname);

    var newValue = data.replace('Panda', 'MyString');

    fs.writeFile('aaa/meo/filelistAsync.ts', newValue, 'utf-8', function (err) {
        if (err) throw err;
        console.log('filelistAsync complete');
    });
});